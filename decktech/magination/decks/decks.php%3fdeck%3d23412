          <td  valign="top"><!-- #BeginEditable "Body" --> 
            <script language="javascript">function card_browser(card)
		{
	var DL;
	DL = "/magination/main/cardbrowser.php?card=" + card; 
	var SITE = window.open(DL,"cardbrowser");
	return;
