                        <table width="100%" border="0" cellspacing="0" cellpadding="0">
                          <tr> 
                            <td bgcolor="#000000" height="8" width="100%" colspan="3"><span class="header">Pasadena, CA 5/12/01</span></td>
                          </tr>
                          <tr> 
                            <td bgcolor="#3399CC" height="2" width="41%"><img src="/stccg/global/images/trek/glear.gif" width="1" height="5"></td>
