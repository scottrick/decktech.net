            <table width="100%" border="0" cellspacing="0" cellpadding="6">
              <tr> 
                <td><!-- #BeginLibraryItem "/Library/Banner2.lbi" --><a href='http://www.decktech.net/adserver/click.php?mgr=aspcode.net&id=22'><img border='0' src='/banner/democomics.gif'  ></a><!-- #EndLibraryItem --></td>
